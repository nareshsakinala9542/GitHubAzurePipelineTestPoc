              </ActivityAction.Argument>
              <Sequence DisplayName="Delete File" sap2010:WorkflowViewState.IdRef="Sequence_1">
                <ui:Delete ContinueOnError="{x:Null}" sap2010:WorkflowViewState.IdRef="Delete_1" Path="[file]" />
              </Sequence>
            </ActivityAction>
          </ui:ForEach.Body>
        </ui:ForEach>
      </If.Then>
